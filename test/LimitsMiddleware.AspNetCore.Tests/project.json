{
  "version": "1.0.0",
  "testRunner": "xunit",

  "buildOptions": {
    "define": [ "LIBLOG_PORTABLE" ],
    "copyToOutput": {
      "include": [ "Files/*.*", "xunit.runner.json" ]
    }
  },

  "dependencies": {
    "coveralls.io": "1.3.4",
    "dotnet-test-xunit": "2.2.0-preview2-build1029",
    "LimitsMiddleware.AspNetCore": { "target": "project" },
    "Shouldly": "2.8.2",
    "OpenCover": "4.6.519",
    "xunit": "2.2.0-beta2-build3300",
    "Microsoft.AspNetCore.StaticFiles": "1.0.0",
    "Microsoft.AspNetCore.TestHost": "1.0.0",
    "System.Reflection": "4.1.0"
  },

  "frameworks": {
    "netcoreapp1.0": {
      "imports": [ "dotnet54" ],
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.0.1"
        }
      }
    }
  }
}
